# =============================================================================
# Agent Runtime - Environment Configuration
# =============================================================================
# Copy this file to .env and update values as needed
# IMPORTANT: Change all default passwords in production!

# -----------------------------------------------------------------------------
# Database
# -----------------------------------------------------------------------------
POSTGRES_PASSWORD=postgres

# -----------------------------------------------------------------------------
# Redis
# -----------------------------------------------------------------------------
# No additional config needed for local dev

# -----------------------------------------------------------------------------
# Object Storage (MinIO)
# -----------------------------------------------------------------------------
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin

# -----------------------------------------------------------------------------
# Identity Provider (Keycloak)
# -----------------------------------------------------------------------------
KEYCLOAK_ADMIN_PASSWORD=admin

# -----------------------------------------------------------------------------
# LangFuse (Agent Observability)
# -----------------------------------------------------------------------------
# Generate secrets: openssl rand -base64 32
LANGFUSE_NEXTAUTH_SECRET=changeme-generate-a-secret
LANGFUSE_SALT=changeme-generate-a-salt
LANGFUSE_PUBLIC_KEY=
LANGFUSE_SECRET_KEY=

# -----------------------------------------------------------------------------
# Observability (Grafana)
# -----------------------------------------------------------------------------
GRAFANA_ADMIN_PASSWORD=admin

# -----------------------------------------------------------------------------
# Application
# -----------------------------------------------------------------------------
DEBUG=true
